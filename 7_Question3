class Pattern1{
	public static void Main(){
		int [][]x;
		x = new int[49][];
		for(int i=0; i<x.Length;i++){
			x[i] = new int[i+1];
		}
		for(int i=0;i<49;i++){
			for(int j=0; j<x[i].Length;j++){
				x[i][j]=j+1;
			}
		}
		for(int i=0;i<49;i++){
			for(int j=0;j<x[i].Length;j++){
				System.Console.Write("{0} ",x[i][j]);
			}
			System.Console.WriteLine();
		}
	}
}